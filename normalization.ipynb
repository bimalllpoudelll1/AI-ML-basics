# X′=(X​−Xmin​)/(Xmax​−Xmin​)
import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt
dataset=pd.read_csv(r"C:\Users\dell\Documents\excells\data1.csv")
dataset.head(3)
dataset.describe()
dataset.isnull().sum()
sns.distplot(dataset["Landsize"])
plt.show()
from sklearn.preprocessing import MinMaxScaler
scale_data=MinMaxScaler()
scale_data.fit(dataset[["Landsize"]])
dataset["Landsize_new"]=pd.DataFrame(scale_data.transform(dataset[["Landsize"]]),columns=["x"])
dataset.head(2)
plt.Figure(figsize=(15,7))
plt.subplot(1,2,1)
plt.title("before")
sns.distplot(dataset[["Landsize"]])

plt.subplot(1,2,2)
plt.title("after")
sns.distplot(dataset[["Landsize_new"]])
plt.show()
